{
  "name": "@grafana/k6-testing",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "nodeModulesDir": "auto",
  "tasks": {
    "build": "deno --allow-env --allow-read --allow-run --allow-sys --allow-ffi='node_modules' --allow-write=./dist tsup.mts",
    "release": "NODE_ENV=production deno --allow-env --allow-read --allow-run --allow-sys --allow-ffi='node_modules' --allow-write=./dist tsup.mts",
    "test": "deno task build && deno run --allow-run --allow-net --allow-read tests/run-tests.ts"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.11",
    "@types/k6": "npm:@types/k6@^0.57.1",
    "k6": "npm:k6@^0.0.0",
    "k6/execution": "./k6-execution-shim.ts",
    "tsup": "npm:tsup@^8.5.0",
    "typescript": "npm:typescript@^5.8.3"
  }
}
